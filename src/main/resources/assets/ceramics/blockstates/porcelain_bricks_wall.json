{
  "multipart": [
    { "when": { "up": "true" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_post" }},
    
    { "when": { "north": "low" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_side", "uvlock": true }},
    { "when": { "east":  "low" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_side", "uvlock": true, "y":  90 }},
    { "when": { "south": "low" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_side", "uvlock": true, "y": 180 }},
    { "when": { "west":  "low" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_side", "uvlock": true, "y": 270 }},
    
    { "when": { "north": "tall" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_side_tall", "uvlock": true }},
    { "when": { "east":  "tall" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_side_tall", "uvlock": true, "y":  90 }},
    { "when": { "south": "tall" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_side_tall", "uvlock": true, "y": 180 }},
    { "when": { "west":  "tall" }, "apply": { "model": "ceramics:block/porcelain_bricks/wall_side_tall", "uvlock": true, "y": 270 }}
  ]
}
